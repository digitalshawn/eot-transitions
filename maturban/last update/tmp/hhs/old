function makeArray(len) {
  for (var i = 0; i < len; i++) this[i] = null; this.length = len;
}

pics = new makeArray(5);
  pics[0] = "<img align='right' src='/images/homepage/gate.jpg' width=120 height=90 alt='girl portrait with iron gate background' border='0'>";
